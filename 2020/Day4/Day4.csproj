  </PropertyGroup>

	<ItemGroup Condition="'$(Configuration)' == 'Debug'">
		<None Update="input.txt" CopyToOutputDirectory="PreserveNewest" />
		<None Update="inputTest.txt" CopyToOutputDirectory="PreserveNewest" />
		<None Update="validTest.txt" CopyToOutputDirectory="PreserveNewest" />
		<None Update="invalidTest.txt" CopyToOutputDirectory="PreserveNewest" />
